    "@types/node": "^12.12.21",
    "@types/pg": "^7.11.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "aws-sdk": "^2.814.0",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
